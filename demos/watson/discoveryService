[Unit]
Description=discoveryService
After=network.target

[Service]
ExecStart=/root/discoveryService/dist/server.bundle.js --max-http-header-size=20000
Restart=always
User=me
Group=group
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/root/discoveryService

[Install]
WantedBy=multi-user.target
